<p id="notice"><%= notice %></p>

<h1>Inter Tables</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Middle name</th>
      <th>Last name</th>
      <th>Address</th>
      <th>Phone no</th>
      <th>Email</th>
      <th>Dob</th>
      <th>Institution</th>
      <th>Major</th>
      <th>Resume</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inter_tables.each do |inter_table| %>
      <tr>
        <td><%= inter_table.First_name %></td>
        <td><%= inter_table.Middle_name %></td>
        <td><%= inter_table.Last_name %></td>
        <td><%= inter_table.Address %></td>
        <td><%= inter_table.Phone_no %></td>
        <td><%= inter_table.Email %></td>
        <td><%= inter_table.DOB %></td>
        <td><%= inter_table.Institution %></td>
        <td><%= inter_table.Major %></td>
        <td><%= inter_table.Resume %></td>
        <td><%= link_to 'Show', inter_table %></td>
        <td><%= link_to 'Edit', edit_inter_table_path(inter_table) %></td>
        <td><%= link_to 'Destroy', inter_table, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inter Table', new_inter_table_path %>
